class Animal:
    def __init__(self, name):
        self.name = name

    def make_sound(self):
        return "Some sound"

class Dog(Animal):  # Dog class inherits from Animal
    def make_sound(self):
        return "Bark"

dog = Dog("Buddy")
print(dog.name)  # Output: Buddy
print(dog.make_sound())  # Output: Bark
